<?xml version="1.0" encoding="ISO-8859-1" ?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema">
	<xs:element name="model" type="modelType" />

	<xs:annotation>
		<xs:documentation xml:lang="nl-NL">Model</xs:documentation>
	</xs:annotation>
	<xs:complexType name="modelType">
		<xs:sequence>
			<xs:element name="Naam" type="xs:string" maxOccurs="1" />
			<xs:element name="Homepage" type="xs:string" maxOccurs="1" />
			<xs:element name="Trefwoorden" type="xs:string" maxOccurs="1" />
			<xs:element name="OwnerID" type="xs:long" maxOccurs="1" />
			<xs:element name="areas" type="areasType" />
			<xs:element name="relations" type="relationsType" />
			<xs:element name="references" type="referencesType" />
			<xs:element name="modules" type="modulesType" />
			<xs:element name="items" type="itemsType" />
			<xs:element name="links" type="linksType" />
			<xs:element name="refitems" type="refitemsType" />
			<xs:element name="coloursetups" type="coloursetupsType" />
			<xs:element name="reports" type="reportsType" />
			<xs:element name="drawcharts" type="drawchartsType" />
	<xs:element name="model" type="modelType" />
		</xs:sequence>
		<xs:attribute name="ID"  use="required" type="xsd:ID" />
		<xs:attribute name="changed" type="xs:date" />
		<xs:attribute name="version" type="xs:string" />
	</xs:complexType>

	<xs:annotation>
		<xs:documentation xml:lang="nl-NL">Aandachtsgebieden</xs:documentation>
	</xs:annotation>
	<xs:element name="areasType" maxOccurs="unbounded">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="area" type="areaType" />
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:complexType name="areaType">
		<xs:sequence>
			<xs:element name="Naam" type="xs:string" maxOccurs="1" />
			<xs:element name="Logo" type="xs:string" maxOccurs="1" />
			<xs:element name="Kolom" maxOccurs="1">
				<xs:simpleType>
					<xs:restriction base="xs:integer">
						<xs:enumeration value="1" />
						<xs:enumeration value="2" />
						<xs:enumeration value="3" />
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:annotation>
				<xs:documentation xml:lang="nl-NL">Positie op pagina</xs:documentation>
			</xs:annotation>

			<xs:element name="Annotatiegebied" type="xs:boolean" maxOccurs="1" />
			<xs:annotation>
				<xs:documentation xml:lang="nl-NL">Een item mag als annotatie aangemaakt worden. Selecteer tijdens het aanpassen een tekst in een editor veld van een ander aandachtsgebied en klik op het annotatie icoon.</xs:documentation>
			</xs:annotation>

			<xs:element name="Begrippenlijst" type="xs:boolean" maxOccurs="1" />
			<xs:annotation>
				<xs:documentation xml:lang="nl-NL">Begrippenlijst. Wanneer de naam van een item uit dit aandachtsgebied op een pagina voorkomt, wordt automatisch een link naar het item aangemaakt.</xs:documentation>
			</xs:annotation>

			<xs:element name="BegrippenCcs" type="xs:string" maxOccurs="1" />
			<xs:element name="AlfabetischSorteren" type="xs:boolean" maxOccurs="1" />
			<xs:annotation>
				<xs:documentation xml:lang="nl-NL">Dit aandachtsgebied altijd alfabetisch sorteren.</xs:documentation>
			</xs:annotation>

			<xs:element name="OvergenomenVanID" type="xs:long" maxOccurs="1" />
			<xs:element name="Homepage" type="xs:string" maxOccurs="1" />
			<xs:element name="OwnerID" type="xs:long" maxOccurs="1" />
			<xs:element name="LogUserID" type="xs:string" maxOccurs="1" />
			<xs:element name="LogFunction" type="xs:string" maxOccurs="1" />
			<xs:element name="areafields" type="areafieldsType" />
			<xs:element name="modules" type="modulesType" />
		</xs:sequence>
		<xs:attribute name="ID"  use="required" type="xsd:ID" />
		<xs:attribute name="changed" type="xs:date" />
	</xs:complexType>

	<xs:annotation>
		<xs:documentation xml:lang="nl-NL">Modules</xs:documentation>
	</xs:annotation>
	<xs:element name="modulesType" maxOccurs="unbounded">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="module" type="moduleType" />
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:complexType name="moduleType">
		<xs:sequence>
			<xs:element name="ModuleCode" maxOccurs="1">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:enumeration value="core" />
						<xs:annotation>
							<xs:documentation xml:lang="nl-NL">Core</xs:documentation>
						</xs:annotation>

						<xs:enumeration value="ModelBeheer" />
						<xs:annotation>
							<xs:documentation xml:lang="nl-NL">Modellenbeheer</xs:documentation>
						</xs:annotation>

						<xs:enumeration value="2" />
						<xs:annotation>
							<xs:documentation xml:lang="nl-NL">Visio-export</xs:documentation>
						</xs:annotation>

						<xs:enumeration value="10" />
						<xs:annotation>
							<xs:documentation xml:lang="nl-NL">Tijdreizen</xs:documentation>
						</xs:annotation>

						<xs:enumeration value="21" />
						<xs:annotation>
							<xs:documentation xml:lang="nl-NL">Analyse</xs:documentation>
						</xs:annotation>

						<xs:enumeration value="23" />
						<xs:annotation>
							<xs:documentation xml:lang="nl-NL">Wet- en regelgeving</xs:documentation>
						</xs:annotation>

						<xs:enumeration value="Rules" />
						<xs:annotation>
							<xs:documentation xml:lang="nl-NL">Hulp bij het schrijven van regels en requirements.</xs:documentation>
						</xs:annotation>

						<xs:enumeration value="Archimate20" />
						<xs:annotation>
							<xs:documentation xml:lang="nl-NL">Archimate 2.0</xs:documentation>
						</xs:annotation>

						<xs:enumeration value="3" />
						<xs:annotation>
							<xs:documentation xml:lang="nl-NL">Business Proces Modelling</xs:documentation>
						</xs:annotation>

						<xs:enumeration value="Annotation" />
						<xs:annotation>
							<xs:documentation xml:lang="nl-NL">Aantekeningen maken die automatisch gekoppeld worden.</xs:documentation>
						</xs:annotation>

						<xs:enumeration value="changelog" />
						<xs:annotation>
							<xs:documentation xml:lang="nl-NL">Wijzigingslog</xs:documentation>
						</xs:annotation>

					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:annotation>
				<xs:documentation xml:lang="nl-NL">Modules</xs:documentation>
			</xs:annotation>

		</xs:sequence>
		<xs:attribute name="ID"  use="required" type="xsd:ID" />
		<xs:attribute name="changed" type="xs:date" />
	</xs:complexType>

	<xs:annotation>
		<xs:documentation xml:lang="nl-NL">Kleurinstellingen</xs:documentation>
	</xs:annotation>
	<xs:element name="coloursetupsType" maxOccurs="unbounded">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="coloursetup" type="coloursetupType" />
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:complexType name="coloursetupType">
		<xs:sequence>
			<xs:element name="Naam" type="xs:string" maxOccurs="1" />
			<xs:element name="colours" type="coloursType" />
		</xs:sequence>
		<xs:attribute name="ID"  use="required" type="xsd:ID" />
		<xs:attribute name="changed" type="xs:date" />
	</xs:complexType>

	<xs:element name="coloursType" maxOccurs="unbounded">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="colour" type="colourType" />
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:complexType name="colourType">
		<xs:sequence>
			<xs:element name="Naam" type="xs:string" maxOccurs="1" />
