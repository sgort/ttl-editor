					<xs:restriction base="xs:integer">
						<xs:enumeration value="1" />
						<xs:annotation>
							<xs:documentation xml:lang="nl-NL">Korte tekst</xs:documentation>
						</xs:annotation>

						<xs:enumeration value="2" />
						<xs:annotation>
							<xs:documentation xml:lang="nl-NL">uitgebreide tekst</xs:documentation>
						</xs:annotation>

						<xs:enumeration value="3" />
						<xs:annotation>
							<xs:documentation xml:lang="nl-NL">Ja/Nee keuzevak</xs:documentation>
						</xs:annotation>

						<xs:enumeration value="4" />
						<xs:annotation>
							<xs:documentation xml:lang="nl-NL">Getal</xs:documentation>
						</xs:annotation>

						<xs:enumeration value="5" />
						<xs:annotation>
							<xs:documentation xml:lang="nl-NL">Geheel getal</xs:documentation>
						</xs:annotation>

						<xs:enumeration value="6" />
						<xs:annotation>
							<xs:documentation xml:lang="nl-NL">Tijd</xs:documentation>
						</xs:annotation>

						<xs:enumeration value="7" />
						<xs:annotation>
							<xs:documentation xml:lang="nl-NL">Datum</xs:documentation>
						</xs:annotation>

						<xs:enumeration value="8" />
						<xs:annotation>
							<xs:documentation xml:lang="nl-NL">Hyperlink</xs:documentation>
						</xs:annotation>

						<xs:enumeration value="9" />
						<xs:annotation>
							<xs:documentation xml:lang="nl-NL">Editor</xs:documentation>
						</xs:annotation>

						<xs:enumeration value="10" />
						<xs:annotation>
							<xs:documentation xml:lang="nl-NL">Keuzelijst</xs:documentation>
						</xs:annotation>

						<xs:enumeration value="11" />
						<xs:annotation>
							<xs:documentation xml:lang="nl-NL">Dashboard</xs:documentation>
						</xs:annotation>

						<xs:enumeration value="13" />
						<xs:annotation>
							<xs:documentation xml:lang="nl-NL">Systeem</xs:documentation>
						</xs:annotation>

					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:annotation>
				<xs:documentation xml:lang="nl-NL">Soort</xs:documentation>
			</xs:annotation>

			<xs:element name="Visibility" maxOccurs="1">
				<xs:simpleType>
					<xs:restriction base="xs:integer">
						<xs:enumeration value="1" />
						<xs:enumeration value="2" />
						<xs:enumeration value="3" />
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:annotation>
				<xs:documentation xml:lang="nl-NL">Zichtbaarheid</xs:documentation>
			</xs:annotation>

			<xs:element name="Verplicht" type="xs:boolean" maxOccurs="1" />
			<xs:annotation>
				<xs:documentation xml:lang="nl-NL">Verplicht</xs:documentation>
			</xs:annotation>

			<xs:element name="Minimum" type="xs:integer" maxOccurs="1" />
			<xs:annotation>
				<xs:documentation xml:lang="nl-NL">Min. aantal tekens</xs:documentation>
			</xs:annotation>

			<xs:element name="Maximum" type="xs:integer" maxOccurs="1" />
			<xs:annotation>
				<xs:documentation xml:lang="nl-NL">Max. aantal tekens</xs:documentation>
			</xs:annotation>

			<xs:element name="AantalTekens" type="xs:integer" maxOccurs="1" />
			<xs:annotation>
				<xs:documentation xml:lang="nl-NL">Max. aantal tekens</xs:documentation>
			</xs:annotation>

			<xs:element name="Veldgrootte" type="xs:integer" maxOccurs="1" />
			<xs:annotation>
				<xs:documentation xml:lang="nl-NL">Aantal tekens per regel</xs:documentation>
			</xs:annotation>

			<xs:element name="Achtervoegsel" type="xs:string" maxOccurs="1" />
			<xs:annotation>
				<xs:documentation xml:lang="nl-NL">Achtervoegsel</xs:documentation>
			</xs:annotation>

			<xs:element name="Opties" type="xs:string" maxOccurs="1" />
			<xs:annotation>
				<xs:documentation xml:lang="nl-NL">Opties</xs:documentation>
			</xs:annotation>

			<xs:element name="UnitChoice" type="xs:string" maxOccurs="1" />
			<xs:annotation>
				<xs:documentation xml:lang="nl-NL">Eenheid kiezen</xs:documentation>
			</xs:annotation>

			<xs:element name="Formula" type="xs:string" maxOccurs="1" />
			<xs:element name="Parameters" type="xs:string" maxOccurs="1" />
			<xs:element name="LogUserID" type="xs:string" maxOccurs="1" />
			<xs:element name="LogFunction" type="xs:string" maxOccurs="1" />
		</xs:sequence>
		<xs:attribute name="ID"  use="required" type="xsd:ID" />
		<xs:attribute name="changed" type="xs:date" />
	</xs:complexType>

	<xs:annotation>
		<xs:documentation xml:lang="nl-NL">Koppelrelaties</xs:documentation>
	</xs:annotation>
	<xs:element name="relationsType" maxOccurs="unbounded">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="relation" type="relationType" />
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:complexType name="relationType">
		<xs:sequence>
			<xs:element name="VanID" type="xs:long" maxOccurs="1" />
			<xs:element name="NaarID" type="xs:long" maxOccurs="1" />
			<xs:element name="Soorten" type="xs:string" maxOccurs="1" />
			<xs:element name="VanAlfabetischSorteren" type="xs:boolean" maxOccurs="1" />
			<xs:element name="NaarAlfabetischSorteren" type="xs:boolean" maxOccurs="1" />
			<xs:element name="LogUserID" type="xs:string" maxOccurs="1" />
			<xs:element name="LogFunction" type="xs:string" maxOccurs="1" />
			<xs:element name="relationfields" type="relationfieldsType" />
		</xs:sequence>
		<xs:attribute name="ID"  use="required" type="xsd:ID" />
		<xs:attribute name="changed" type="xs:date" />
	</xs:complexType>

	<xs:annotation>
		<xs:documentation xml:lang="nl-NL">Velden</xs:documentation>
	</xs:annotation>
	<xs:element name="relationfieldsType" maxOccurs="unbounded">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="relationfield" type="relationfieldType" />
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:complexType name="relationfieldType">
		<xs:sequence>
			<xs:element name="Naam" type="xs:string" maxOccurs="1" />
			<xs:element name="KleurenschemaID" type="xs:long" maxOccurs="1" />
			<xs:element name="Soort" maxOccurs="1">
				<xs:simpleType>
					<xs:restriction base="xs:integer">
						<xs:enumeration value="1" />
